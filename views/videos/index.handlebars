{{#if session.user}}
{{session.user}}, <a href="/logout">Log out</a>
{{/if}}
<div class="contents-container" id="videos-container">
  {{#each videos as |video|}}
  <div class="video-card">
    <div class="video-title">
      <a href="/videos/{{video._id}}">{{video.title}}</a>
    </div>
    <iframe class="video-player" src="{{video.videoUrl}}" frameborder="0" allowfullscreen></iframe>
  </div>
  {{/each}}
</div>

<a href="/videos/create.html" class="add-video-button">
  <div>
    <p>Add Video</p>
  </div>
</a>

{{#unless session.user}}
<a href="/login" class="input-button">
  <div>
    <p>Log in</p>
  </div>
</a>
{{/unless}}
